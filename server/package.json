  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "openai": "^5.19.1",
    "puppeteer": "^24.19.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
